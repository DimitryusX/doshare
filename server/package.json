{
  "name": "fdata-server",
  "version": "0.1.1",
  "description": "",
  "main": "build/main.js",
  "scripts": {
    "build": "npx tsc",
    "start": "node --max-old-space-size=4096 build/main.js",
    "dev": "concurrently \"npx tsc --watch\" \"nodemon -q build/main.js\"",
    "debug": "concurrently \"npx tsc --watch\" \"node build/main.js\"",
    "clean": "rm -rf ./build && mkdir build",
    "test": "jest",
    "test:e2e": "jest --testPathPattern=src/e2e",
    "sentry:sourcemaps": "sentry-cli sourcemaps inject --org nexol --project doshare-server ./build && sentry-cli sourcemaps upload --org nexol --project doshare-server ./build"
  },
  "author": "",
  "license": "ISC",
