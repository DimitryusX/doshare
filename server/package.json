    "bcryptjs": "^3.0.2",
    "connect-busboy": "^1.0.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "express-validator": "^7.2.1",
    "ioredis": "^5.5.0",
    "joi": "^17.13.3",
    "mongoose": "^8.11.0",
    "morgan": "^1.10.0",
    "multer": "1.4.5-lts.1",
    "stream-meter": "^1.0.4"
  },
  "devDependencies": {
